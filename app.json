{
  "expo": {
    "name": "CoupleTasks",
    "slug": "CoupleTasks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eden.coupletasks"
    },
    "android": {
      "package": "com.eden.coupletasks",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "coupletasks",
    "extra": {
      "googleClientId": "454424635796-1shld82shvpp46trb6q566crjp48qtf9.apps.googleusercontent.com"
    },
    "plugins": [
      "expo-localization",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#8B5CF6",
          "sounds": []
        }
      ]
    ]
  }
}